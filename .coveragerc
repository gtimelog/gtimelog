[run]
source = src/gtimelog
omit =
    src/gtimelog/main.py
    src/gtimelog/paths.py
    src/gtimelog/debian-paths.py
    src/gtimelog/tests/*
cover_pylib = False
plugins =
    coverage_python_version

[report]
exclude_lines =
    pragma: nocover
    if __name__ == '__main__':
    except ImportError:
    except NameError:
